<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
        "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="SampleGwtApplication">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.southbanksoftware.samplegwtapplication.client.SampleGwtApplication'/>


    <!-- Specify the app servlets.                   -->
    <servlet path='/SampleGwtApplicationService'
             class='com.southbanksoftware.samplegwtapplication.server.SampleGwtApplicationServiceImpl'/>

</module>